{
  "name": "@decentm/allegiance-chess-bot",
  "version": "0.0.1",
  "scripts": {
    "t": "c8 ava --timeout 5m",
    "ingest-openings": "ts-node --transpile-only -r tsconfig-paths/register tools/opening-ingestion.ts",
    "dev": "ts-node-dev --transpile-only --clear -r tsconfig-paths/register",
    "clinic": "TS_NODE_PROJECT=tsconfig.spec.json clinic",
    "build:clinic": "tsc -p tsconfig.lib.json && sed -i 's/@decentm\\/allegiance-chess-core/..\\/..\\/core\\/src/g' ../../dist/out-tsc/packages/bot/clinic/self-play.js && sed -i 's/@decentm\\/allegiance-chess-core/..\\/..\\/..\\/core\\/src/g' ../../dist/out-tsc/packages/bot/src/engine/index.js",
    "dev:clinic": "yarn build:clinic && node ../../dist/out-tsc/packages/bot/clinic/self-play.js",
    "flame": "yarn clinic flame --kernel-tracing -- -- node -r ts-node/register/transpile-only -r tsconfig-paths/register",
    "doctor": "yarn clinic doctor -- -- node -r ts-node/register/transpile-only -r tsconfig-paths/register",
    "bubbleprof": "yarn clinic bubbleprof -- -- node -r ts-node/register/transpile-only -r tsconfig-paths/register",
    "heapprofiler": "yarn clinic heapprofiler -- -- node -r ts-node/register/transpile-only -r tsconfig-paths/register"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "tsconfig-paths/register"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.spec.json"
    }
  },
  "dependencies": {
    "@decentm/allegiance-chess-core": "0.x",
    "seedrandom": "^3.0.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "clinic": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}